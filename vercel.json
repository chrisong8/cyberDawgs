{
    "version": 2,
    "builds": [
      {
        "src": "src/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*).html",
        "headers": { "cache-control": "public,max-age=0,must-revalidate" },
        "dest": "/$1.html"
      },
      {
        "src": "/(.*).(js|css)",
        "headers": { "cache-control": "public,max-age=31536000,immutable" },
        "dest": "/$1.$2"
      },
      {
        "src": "/static/(.*)",
        "headers": { "cache-control": "public,max-age=31536000,immutable" },
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "headers": { "cache-control": "public,max-age=0,must-revalidate" },
        "dest": "/index.html"
      }
    ]
  }
  